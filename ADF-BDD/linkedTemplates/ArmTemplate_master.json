{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-BDD"
		},
		"AzureBlobStorageLand_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageLand'"
		},
		"AzureBlobStorageLand2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageLand2'"
		},
		"AzurePrftTutorialADF_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzurePrftTutorialADF'"
		},
		"slstOrders_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'slstOrders'"
		},
		"lsdlOrders_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'lsdlOrders'"
		},
		"lsdlOrders_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://prftbibddadlsa.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-BDD_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorageLand_connectionString": {
						"value": "[parameters('AzureBlobStorageLand_connectionString')]"
					},
					"AzureBlobStorageLand2_connectionString": {
						"value": "[parameters('AzureBlobStorageLand2_connectionString')]"
					},
					"AzurePrftTutorialADF_connectionString": {
						"value": "[parameters('AzurePrftTutorialADF_connectionString')]"
					},
					"slstOrders_connectionString": {
						"value": "[parameters('slstOrders_connectionString')]"
					},
					"lsdlOrders_accountKey": {
						"value": "[parameters('lsdlOrders_accountKey')]"
					},
					"lsdlOrders_properties_typeProperties_url": {
						"value": "[parameters('lsdlOrders_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ADF-BDD_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADF-BDD_ArmTemplate_0')]"
			]
		},
		{
			"name": "ADF-BDD_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADF-BDD_ArmTemplate_1')]"
			]
		}
	]
}